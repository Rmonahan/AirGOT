<!DOCTYPE html>
<html>
  <head>
    <title>AirGoT</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
     <%= javascript_include_tag "https://maps.googleapis.com/maps/api/js?key=#{ENV['MAPS_API_KEY']}" %>
    <%= stylesheet_link_tag    'application', media: 'all' %>

    <%= javascript_include_tag 'application' %>
    <script src="https://kit.fontawesome.com/dcce58ae7c.js" crossorigin="anonymous"></script>
    <script type=text/javascript>
    
      window.backgroundImage = "<%= image_url('background.jpg') %>"
      window.logoImage = "<%= image_url('logo.png') %>"
      window.logoName = "<%= image_url('logo-with-name.png') %>"
    </script>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
